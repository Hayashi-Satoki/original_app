<div class="contents row">
  <div class="container">
    <h3>投稿する</h3>
    <%= form_with(model: @picture, local: true) do |form| %>
      <%= form.text_field :family_id, placeholder: "Familyname" %>
      <%= form.text_area :description, placeholder: "text", rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>
